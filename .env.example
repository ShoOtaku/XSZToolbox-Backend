# 服务器配置
PORT=3000
NODE_ENV=development

# 数据库配置
DATABASE_PATH=./database/xsztoolbox.db

# JWT 配置
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=24h

# 管理员账号配置（用于初始化默认管理员）
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change_me_immediately

# HMAC 签名密钥（与前端插件共享）
HMAC_SECRET=your-hmac-secret-key-change-this

# CID 哈希盐值（必须与前端插件一致）
HASH_SALT=XSZToolbox_CID_Salt_2025

# 管理员 CID 哈希列表（逗号分隔）
ADMIN_CID_HASHES=A7F3C9E2D4B1F8A6EXAMPLE1,B8G4D0F3E5C2G9B7EXAMPLE2

# 安全配置
REQUIRE_HTTPS=true
ENABLE_RATE_LIMIT=true
MAX_REQUESTS_PER_MINUTE=60

# CORS 配置
CORS_ORIGIN=*

# 日志配置
LOG_LEVEL=info
LOG_FILE_ENABLED=true
